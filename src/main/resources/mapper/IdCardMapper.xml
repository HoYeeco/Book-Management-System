<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="github.hoyeeco.bookmanagementsystem.mapper.IdCardMapper">
    <select id="findIdCardById"
            resultType="github.hoyeeco.bookmanagementsystem.pojo.IdCard"
            parameterType="Integer">
        select *
        from t_idcard
        where id = #{id}
    </select>
</mapper>
